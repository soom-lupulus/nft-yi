generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Nft {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  name   String  @unique
  price  String?
  rating Float   @default(4.5)
}
